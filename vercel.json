{
    "version": 2,
    "builds": [
        {
            "src": "FootFront/wsgi.py",
            "use": "@vercel/python",
            "config": { "includeFiles": ["initial_data.json"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "FootFront/wsgi.py"
        }
    ]
}
